{
    "manifest_version": 2,
    "name": "Wikit",
    "version": "1.0",
    "description": "Search Wikipedia on the go",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png",
        "32": "icon32.png"
    },
    "background": {
        "scripts": ["eventPage.js"],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "contextMenus"
    ]
}